



  <div class="container">
  {{#each docs}}
          
          <div class="row">
            <div class="col-md-4">
              <div class="card mb-4 box-shadow">
                <img class="card-img-top" src="{{this.imageUrl}}" alt="Card image cap">
                <div class="card-body">
                  <p class="card-text">Price: {{this.price}}</p>
                  <div class="d-flex justify-content-between align-items-center">
                    <div class="btn-group">
                      <form action="/admin/admin-product-list/{{this._id}}/delete" method="POST"></form><button type="button" class="btn btn-sm btn-outline-secondary">Delete</button> </form>
                      <button type="button" class="btn btn-sm btn-outline-secondary"><a class="btn btn-primary" href="/admin/admin-product-edit/edit?product_id={{this._id}}" class="edit-button">EDIT</a> </button>
                    </div>                    
                  </div>
                </div>
              </div>
            </div>
            </div>   
      {{/each}}
  </div>        
        
